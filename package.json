  "bugs": {
    "url": "https://github.com/kapouer/upcache/issues"
  },
  "dependencies": {
    "cookie": "^0.3.1",
    "dashdash": "^1.14.0",
    "debug": "^2.6.9",
    "express-cache-response-directive": "^1.1.0",
    "http-errors": "^1.6.2",
    "jsonwebtoken": "^8.1.1"
  },
  "devDependencies": {
    "express": "^4.16.2",
    "mocha": "^4",
    "should": "^13"
  }
